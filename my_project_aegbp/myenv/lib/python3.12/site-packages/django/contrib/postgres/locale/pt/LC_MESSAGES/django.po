#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Item %(nth)s na lista não validou:"

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "As sub-listas têm de ter o mesmo tamanho."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Mapeamento de strings para strings/nulos"

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr ""

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Não foi possível carregar os dados JSON."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "A entrada deve ser um dicionário JSON."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Introduza dois valores válidos."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "O início da gama não pode ser maior que o seu fim."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Introduza dois números inteiros."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Introduza dois números."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Introduza duas datas/horas válidas."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Introduza duas datas válidas."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"A lista contém %(show_value)d item, não pode conter mais do que "
"%(limit_value)d."
msgstr[1] ""
"A lista contém %(show_value)d itens, não pode conter mais do que "
"%(limit_value)d."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"A lista contém %(show_value)d item, tem de conter pelo menos %(limit_value)d."
msgstr[1] ""
"A lista contém %(show_value)d itens, tem de conter pelo menos "
"%(limit_value)d."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Algumas chaves estão em falta: %(keys)s"

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Foram fornecidas algumas chaves desconhecidas: %(keys)s"

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:81
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely greater than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr "Garanta que esta gama é toda ela maior ou igual a %(limit_value)s."

#: myenv/lib/python3.12/site-packages/django/contrib/postgres/validators.py:90
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely less than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr "Garanta que esta gama é toda ela menor ou igual a %(limit_value)s."
