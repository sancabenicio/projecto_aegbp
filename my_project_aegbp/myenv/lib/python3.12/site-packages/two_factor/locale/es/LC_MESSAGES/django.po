#: myenv/lib/python3.12/site-packages/two_factor/plugins/webauthn/forms.py:140
msgid "Entered token is not valid."
msgstr "El código introducido no es válido."

#: myenv/lib/python3.12/site-packages/two_factor/forms.py:105
msgid "Yes, I am sure"
msgstr "Sí, estoy seguro"

#: myenv/lib/python3.12/site-packages/two_factor/forms.py:143
#, python-format
msgid "Don't ask again on this device for %(minutes)i minutes"
msgstr "No preguntar de nuevo en este dispositivo por %(minutes)iminutos"

#: myenv/lib/python3.12/site-packages/two_factor/forms.py:146
#, python-format
msgid "Don't ask again on this device for %(hours)i hours"
msgstr "No preguntar de nuevo en este dispositivo por %(hours)ihoras"

#: myenv/lib/python3.12/site-packages/two_factor/forms.py:149
#, python-format
msgid "Don't ask again on this device for %(days)i days"
msgstr "No preguntar de nuevo en este dispositivo por %(days)i días"

